<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>$(NetCoreAppCurrent)</TargetFramework>
  </PropertyGroup>

  <Target Name="TestMessage"  AfterTargets="Build" >
    <Message Text="TargetingpacksTargetsImported: $(TargetingpacksTargetsImported)" Importance="High" />
    <Message Text="NetCoreAppCurrent: $(NetCoreAppCurrent)" Importance="High" />

    <Message Text="TargetFrameworkVersion: $(TargetFrameworkVersion)" Importance="High" />
    <Message Text="NetCoreAppCurrentVersion: $(NetCoreAppCurrentVersion)" Importance="High" />
    
    <Message Text="DisableImplicitFrameworkReferences: $(DisableImplicitFrameworkReferences)" Importance="High" />
    <Message Text="TargetFrameworkIdentifier: $(TargetFrameworkIdentifier)" Importance="High" />

    <Message Text="MicrosoftNetCoreAppRuntimePackDir: $(MicrosoftNetCoreAppRuntimePackDir)" Importance="High" />
    <Message Text="MicrosoftNetCoreAppRuntimePackNativeDir: $(MicrosoftNetCoreAppRuntimePackNativeDir)" Importance="High" />
    <Message Text="NetCoreAppCurrentRefPath: $(NetCoreAppCurrentRefPath)" Importance="High" />
    <Message Text="NetCoreAppCurrentRuntimePath: $(NetCoreAppCurrentRuntimePath)" Importance="High" />
    <Message Text="NetCoreAppCurrentTargetFrameworkMoniker: $(NetCoreAppCurrentTargetFrameworkMoniker)" Importance="High" />

    <Message Text="TargetRid: $(TargetRid)" Importance="High" />

    <Message Text="RuntimeOS: $(RuntimeOS)" Importance="High" />
  </Target>
</Project>
